ADMIN_USER=admin
ADMIN_PASSWORD=admin

# OpenTelemetry Configuration
# OTEL_EXPORTER_OTLP_ENDPOINT: The endpoint of the OTEL collector (without http://)
# This endpoint is used for both metrics and traces via OTLP HTTP protocol
# For Docker networks, use the service name: otel-collector:4318
# For local development, use: localhost:4318
OTEL_EXPORTER_OTLP_ENDPOINT=otel-collector:4318

# OTEL_SERVICE_NAME: The name of this service in telemetry data
OTEL_SERVICE_NAME=gourmet-app

# OTEL_SERVICE_VERSION: The version of this service
OTEL_SERVICE_VERSION=1.0.0
